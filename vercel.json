{
    "builds": [
        {
            "src": "api/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/ads.txt",
            "dest": "/app/templates/ads.txt"
        },
        {
            "src": "/(.*)",
            "dest": "api/wsgi.py"
        }
    ]
}
